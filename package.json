    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "backstopjs": "^3.5.1",
    "browserslist": "^2.4.0",
    "classlist-polyfill": "^1.2.0",
    "clean-webpack-plugin": "^0.1.17",
    "connect": "^3.6.6",
    "css-loader": "^0.28.7",
    "escape-html": "^1.0.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsdoc": "^3.1.3",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-require-jsdoc": "^1.0.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "front-matter": "^2.3.0",
    "frontmatter": "0.0.3",
    "highlightjs": "^9.10.0",
    "html": "^1.0.0",
    "html-entities": "^1.2.1",
    "html-loader": "^0.5.5",
    "jest": "^21.2.1",
    "markdown-it": "^8.4.2",
    "markdown-it-highlightjs": "^3.0.0",
    "marked": "^0.3.16",
    "marked-extras": "^0.2.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "opn": "^5.2.0",
    "postcss-loader": "^2.1.6",
    "postcss-normalize": "^4.0.0",
    "preact-render-to-string": "^3.7.0",
    "prop-types": "^15.6.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "serve-static": "^1.13.2",
    "style-loader": "^0.18.2",
    "stylelint": "^8.1.1",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-no-unsupported-browser-features": "^1.0.0",
    "stylelint-scss": "^2.1.0",
    "webpack": "^3.8.1",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-md5-hash": "0.0.5"
  },
  "browserslist": [
    "last 12 chrome versions",
    "last 12 firefox versions",
    "last 6 safari versions",
    "explorer >= 10",
    "edge > 0"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "plugins": [
      "require-jsdoc",
      "jsdoc",
      "compat"
    ],
    "settings": {
      "polyfills": [
        "fetch",
        "promises"
      ]
    },
    "extends": "airbnb",
    "rules": {
      "require-jsdoc": 0,
      "jsdoc/check-param-names": 1,
      "jsdoc/check-tag-names": 1,
      "jsdoc/check-types": 1,
      "jsdoc/newline-after-description": 1,
      "jsdoc/require-description-complete-sentence": 0,
      "jsdoc/require-example": 0,
      "jsdoc/require-hyphen-before-param-description": 1,
      "jsdoc/require-param": 1,
      "jsdoc/require-param-description": 1,
      "jsdoc/require-param-type": 1,
      "jsdoc/require-returns-description": 1,
      "jsdoc/require-returns-type": 1,
      "compat/compat": 1,
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/href-no-hash": 0,
      "linebreak-style": 0,
      "arrow-body-style": 0,
      "import/prefer-default-export": 0,
      "react/react-in-jsx-scope": 0,
      "react/no-unknown-property": [
        2,
        {
          "ignore": [
            "allowfullscreen"
          ]
        }
      ],
      "no-plusplus": 0,
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "h"
        }
      ],
      "max-len": 0
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-css-modules"
    ],
    "plugins": [
      "stylelint-scss",
      "stylelint-no-unsupported-browser-features"
    ],
    "rules": {
      "plugin/no-unsupported-browser-features": [
        true,
        {
          "severity": "warning"
        }
      ],
      "at-rule-empty-line-before": [
        "never",
        {
          "ignoreAtRules": [
            "media",
            "import",
            "if",
            "for"
          ]
        }
      ],
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "import-normalize",
            "if",
            "return",
            "function",
            "mixin",
            "include"
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "env",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "array.from": "^1.0.3",
    "array.prototype.every": "^1.0.0",
    "array.prototype.findindex": "^2.0.2",
    "camelcase": "^4.1.0",
    "canvg-browser": "^1.0.0",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.5",
    "lunr": "^2.1.6",
    "object.assign": "^4.1.0",
    "preact": "^8.2.6",
    "preact-css-transition-group": "^1.3.0",
    "preact-transition-group": "^1.1.1",
    "promise-polyfill": "^6.0.2",
    "query-string": "^5.1.1",
    "react-html-connector": "^0.1.21",
    "redux": "^4.0.0",
    "save-svg-as-png": "^1.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
